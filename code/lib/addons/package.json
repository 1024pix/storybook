    "prep": "../../../scripts/prepare/bundle.ts"
  },
  "dependencies": {
    "@storybook/preview": "7.0.0-alpha.45"
  },
  "publishConfig": {
    "access": "public"
  },
  "bundler": {
    "entries": [
      "./src/entry.ts"

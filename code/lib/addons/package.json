{
  "name": "@storybook/addons",
  "version": "7.0.0-alpha.45",
  "description": "Storybook addons store",
  "keywords": [
    "storybook"
  ],
  "homepage": "https://github.com/storybookjs/storybook/tree/main/lib/addons",
  "bugs": {
    "url": "https://github.com/storybookjs/storybook/issues"
  },
